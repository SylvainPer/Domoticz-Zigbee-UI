{
  "name": "zigate-plugin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "ngcc --tsconfig 'tsconfig.app.json' && npm run prettier -s",
    "ng": "ng",
    "buildPourReverse": "npm run env -s && ng build --configuration production --base-href /zigate/ --deploy-url /zigate/",
    "build": "npm run env -s && ng build --configuration production",
    "start": "npm run env -s && ng serve --proxy-config proxy.conf.js",
    "start1": "npm run env -s && ng serve",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort --marker extract",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,html,scss}\"",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^12.2.8",
    "@angular/cdk": "^12.2.8",
    "@angular/common": "^12.2.8",
    "@angular/compiler": "^12.2.8",
    "@angular/core": "^12.2.8",
    "@angular/forms": "^12.2.8",
    "@angular/localize": "^12.2.8",
    "@angular/platform-browser": "^12.2.8",
    "@angular/platform-browser-dynamic": "^12.2.8",
    "@angular/router": "^12.2.8",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@ng-select/ng-option-highlight": "0.0.6",
    "@ng-select/ng-select": "^6.1.0",
    "@ngx-translate/core": "^13.0.0 ",
    "@swimlane/ngx-charts": "^17.0.1",
    "@swimlane/ngx-datatable": "^19.0.0",
    "@types/file-saver": "^2.0.2",
    "angular-highcharts": "^11.3.1",
    "bootstrap": "^4.5.0",
    "browserslist": "^4.16.6",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.12.1",
    "file-saver": "^2.0.5",
    "highcharts": "^9.1.0",
    "mousetrap": "^1.6.5",
    "ngx-color-picker": "^11.0.0",
    "ngx-filesaver": "^11.0.0",
    "ngx-json-viewer": "^3.0.2",
    "ngx-mousetrap": "^11.0.0",
    "ngx-perfect-scrollbar": "^10.1.1",
    "ngx-spinner": "^11.0.1",
    "ngx-toastr": "^13.2.1",
    "rxjs": "^6.6.7",
    "subsink": "^1.0.2",
    "toppy": "^2.3.4",
    "tslib": "^2.2.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.8",
    "@angular/cli": "^12.2.8",
    "@angular/compiler-cli": "^12.2.8",
    "@angular/language-service": "^12.2.8",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@ngx-rocket/scripts": "^5.1.0",
    "@types/jasmine": "^3.7.2",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^15.0.2",
    "codelyzer": "^6.0.2",
    "htmlhint": "^0.14.2",
    "https-proxy-agent": "^5.0.0",
    "husky": "^6.0.0",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "karma-junit-reporter": "^2.0.1",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "stylelint": "~13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "~4.2.0",
    "stylelint-config-standard": "~22.0.0",
    "stylelint-scss": "~3.19.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "semi": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --ignore-path=.prettierignore"
    }
  }
}
